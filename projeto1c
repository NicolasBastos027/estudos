#include <stdio.h>
int main () //função principal
{
    float a,b,c,d,e;//declaração de variaveis
    printf ("digite a primeira nota:");
    scanf("%f",&a);
    
    printf("digite a segunda nota:");
    scanf ("%f",&b);
    
    printf("digite a terceira nota:");
    scanf ("%f",&c);
    
    printf("digite a quarta nota:");
    scanf ("%f",&d);
    
    e= (a + b + c + d)/4;
    printf ("a media dentre as notas %.2f, %.2f, %.2f, %.2f é %.2f", a, b, c,d, e);
    return 0;
}
